function generateGrade() {
  const marks = parseInt(prompt("Enter the student's marks (0-100):"), 10);

  if (isNaN(marks) || marks < 0 || marks > 100) {
    return "Invalid input! Please enter a number between 0 and 100.";
  }

  if (marks > 79) {
    return "Grade: A";
  } else if (marks >= 60) {
    return "Grade: B";
  } else if (marks >= 50) {
    return "Grade: C";
  } else if (marks >= 40) {
    return "Grade: D";
  } else {
    return "Grade: E";
  }
}

